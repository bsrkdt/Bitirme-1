<Window x:Class="deneme.KitchenStaff"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:deneme"
        mc:Ignorable="d"
        Title="KitchenStaff" Height="450" Width="800">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="306" Margin="0,104,-0.4,0" VerticalAlignment="Top" Width="794"/>
        <DatePicker HorizontalAlignment="Left" Height="31" Margin="661,10,0,0" VerticalAlignment="Top" Width="123"/>
        <Button Content="Update Page" HorizontalAlignment="Left" Height="31" Margin="520,8,0,0" VerticalAlignment="Top" Width="123">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
            </Button.Background>
        </Button>
        <Button Content="Delete the Order" HorizontalAlignment="Left" Height="31" Margin="371,8,0,0" VerticalAlignment="Top" Width="123">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
